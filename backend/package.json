{
  "name": "backend",
  "version": "1.0.0",
  "description": "Task Management Backend API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch dist --exec \"node dist/server.js\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "watch": "tsc -w",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules\" jest --coverage",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "node dist/scripts/migrate.js",
    "seed": "node dist/scripts/seed.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.0"
  },
  "keywords": [
    "express",
    "typescript",
    "sequelize",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
